<g class="quad">
  <circle bp-connector="default" cx="0" cy="0" r="25"/>  
  <circle bp-connector="ret" cx="-25" cy="0" r="13"/>
  <text class="quad-connector" x="-25" y="3" text-anchor="middle">RET</text>
  <circle bp-connector="por" cx="+25" cy="0" r="13"/>
  <text class="quad-connector" x="+25" y="3" text-anchor="middle">POR</text>
  <circle bp-connector="sub" cx="0" cy="-25" r="13"/>
  <text class="quad-connector" x="0" y="-22" text-anchor="middle">SUB</text>
  <circle bp-connector="sur" cx="0" cy="+25" r="13"/>
  <text class="quad-connector" x="0" y="+28" text-anchor="middle">SUR</text>
  <text class="label" x="0" y="50" text-anchor="middle">{{data.label}}</text>
</g>